programa {
  funcao inicio() {

    real num1, num2, num3, media_final

    escreva ("Digite a primeira nota: ")
    leia (num1)

    escreva ("Digite a segunda nota: ")
    leia (num2)

    escreva ("Digite a terceira  nota: ")
    leia (num3)

    media_final = (num1*2)+(num2*3)+(num3*5)/10

    escreva ("Sua média final será de: ", media_final)
  }
}
